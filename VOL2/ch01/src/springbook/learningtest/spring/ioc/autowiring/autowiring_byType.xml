<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd"  default-autowire="byType">

    <bean id ="hello" class="springbook.learningtest.spring.ioc.autowiring.Hello">
        <property name="name" value="Spring"/>
    </bean>
    <bean id="mainPrinter" class="springbook.learningtest.spring.ioc.autowiring.StringPrinter"/>

</beans>